{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["DOM", "ESNext"],
    "useDefineForClassFields": false,
    "newLine": "lf",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "removeComments": false,
    "jsx": "preserve",
    "strict": true,
    "strictNullChecks": true,
    "sourceMap": false,
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "dist",
    "paths": {
      "@vue/*": ["packages/@vue/*/src"],
      // "vue": ["packages/vue/src"]
    }
  }
}